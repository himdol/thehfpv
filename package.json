{
  "name": "github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://himdol.github.io/",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "kill-ports": "npx kill-port 3000 8080 8081 8082",
    "dev": "npm run kill-ports && concurrently \"npm start\" \"cd backend && ./gradlew bootRun\"",
    "dev:backend": "cd backend && ./gradlew bootRun",
    "dev:frontend": "npm start",
    "install:all": "npm install && cd backend && ./gradlew build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  }
}
